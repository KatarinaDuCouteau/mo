{
  "compilerOptions": {
//    "declaration": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./lib",
    "lib": [
      "ES5",
      "ES2015.Core",
      "ES2015.Collection",
      "ES2015.Promise",
      "DOM"
    ],
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@jupyterlab/*": [
        "../*/src"
      ],
      "@jupyterlab/mainmenu-extension": [
        "../mainmenu-extension/src"
      ],
      "@jupyterlab/modules-extension": [
        "../modules-extension/src"
      ]
    },
    "jsx": "react",
    "inlineSources": true,
    "allowJs": true,
    "sourceMap": true
  }
}
